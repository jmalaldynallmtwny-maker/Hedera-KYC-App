{
  "name": "kyc-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc --incremental false",
    "start": "node dist/index.js",
    "db:generate": "npm run db:generate:all",
    "db:generate:citizens": "prisma generate --schema prisma/schema/citizens/schema.prisma",
    "db:generate:baybank": "prisma generate --schema prisma/schema/baybank/schema.prisma",
    "db:generate:oasisbank": "prisma generate --schema prisma/schema/oasisbank/schema.prisma",
    "db:generate:zenbank": "prisma generate --schema prisma/schema/zenbank/schema.prisma",
    "db:generate:arcbank": "prisma generate --schema prisma/schema/arcbank/schema.prisma",
    "db:generate:nexbank": "prisma generate --schema prisma/schema/nexbank/schema.prisma",
    "db:generate:all": "npm run db:generate:citizens && npm run db:generate:baybank && npm run db:generate:oasisbank && npm run db:generate:zenbank && npm run db:generate:arcbank && npm run db:generate:nexbank",
    "db:migrate": "npm run db:migrate:all",
    "db:migrate:citizens": "prisma migrate deploy --schema prisma/schema/citizens/schema.prisma",
    "db:migrate:baybank": "prisma migrate deploy --schema prisma/schema/baybank/schema.prisma",
    "db:migrate:oasisbank": "prisma migrate deploy --schema prisma/schema/oasisbank/schema.prisma",
    "db:migrate:zenbank": "prisma migrate deploy --schema prisma/schema/zenbank/schema.prisma",
    "db:migrate:arcbank": "prisma migrate deploy --schema prisma/schema/arcbank/schema.prisma",
    "db:migrate:nexbank": "prisma migrate deploy --schema prisma/schema/nexbank/schema.prisma",
    "db:migrate:all": "npm run db:migrate:citizens && npm run db:migrate:baybank && npm run db:migrate:oasisbank && npm run db:migrate:zenbank && npm run db:migrate:arcbank && npm run db:migrate:nexbank",
    "db:push": "npm run db:push:all",
    "db:push:citizens": "prisma db push --schema prisma/schema/citizens/schema.prisma --skip-generate",
    "db:push:baybank": "prisma db push --schema prisma/schema/baybank/schema.prisma --skip-generate",
    "db:push:oasisbank": "prisma db push --schema prisma/schema/oasisbank/schema.prisma --skip-generate",
    "db:push:zenbank": "prisma db push --schema prisma/schema/zenbank/schema.prisma --skip-generate",
    "db:push:arcbank": "prisma db push --schema prisma/schema/arcbank/schema.prisma --skip-generate",
    "db:push:nexbank": "prisma db push --schema prisma/schema/nexbank/schema.prisma --skip-generate",
    "db:push:all": "npm run db:push:citizens && npm run db:push:baybank && npm run db:push:oasisbank && npm run db:push:zenbank && npm run db:push:arcbank && npm run db:push:nexbank && npm run db:generate:all",
    "db:seed": "tsx src/scripts/seedAllData.ts",
    "docker:build": "docker build -t kyc-backend .",
    "docker:run": "docker run -p 3000:3000 kyc-backend",
    "test": "vitest run",
    "lint": "eslint src/**/*.{ts,tsx}",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@hashgraph/sdk": "^2.49.0",
    "@prisma/client": "^5.22.0",
    "@types/form-data": "^2.2.1",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "bullmq": "^4.12.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "form-data": "^4.0.4",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "minio": "^7.1.3",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "openai": "^4.68.0",
    "tsx": "^4.18.1",
    "uuid": "^9.0.1",
    "winston": "^3.14.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/csurf": "^1.11.5",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.9",
    "@types/node": "^22.10.1",
    "@types/node-fetch": "^2.6.11",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^8.57.1",
    "prisma": "^5.22.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.6"
  }
}
